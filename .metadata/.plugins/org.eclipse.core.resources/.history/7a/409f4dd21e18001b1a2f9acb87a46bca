package programmers.level1;

import java.util.Arrays;

public class Solution_문자열내림차순으로배치하기 {
    public String solution(String s) {
    	Character[] carr = new Character[s.length()];
    	for (int i = 0; i < carr.length; i++) carr[i] = s.charAt(i);
    	
    	Arrays.sort(carr, (c1, c2) -> -(c1-c2));
    	System.out.println(String.valueOf(carr));
		
    	return null;
    }
    
    public static void main(String[] args) {
    	Solution_문자열내림차순으로배치하기 s = new Solution_문자열내림차순으로배치하기();
		System.out.println(s.solution("Zbcdefg"));
	}
}
